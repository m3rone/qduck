{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "additionalProperties": {
        "type": "object",
        "properties": {
            "alias": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "minItems": 1
            },
            "prompt": {
                "type": "string"
            }
        },
        "required": ["alias", "prompt"]
    }
}
